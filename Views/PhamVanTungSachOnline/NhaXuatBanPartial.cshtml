@model IEnumerable<PhamVanTung.SachOnline.Models.NHAXUATBAN>

<ul class="list-group" style="margin-top:15px;">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">SACH THEO NHA XUAT BAN</li>
    @if (Model != null)
    {
        foreach (var nxb in Model)
        {
            <li class="list-group-item">
                <a href="@Url.Action("SachTheoNXB", "PhamVanTungSachOnline", new { id = nxb.MaNXB })">
                    @nxb.TenNXB
                </a>
            </li>
        }
    }
</ul>
